#include<stdio.h>

double power(double n, int p);
//n^p= n^p/2*n^p/2*n

int main(void){



return 0;

}

double power(double n, int p){
double ans;
if(p==0){
return 1.0;}

ans = power(n, p/2); //還沒return;
if(p%2==0){
return ans*ans;
}
else{
return n*ans*ans;
}
}

____________________________
#include<stdio.h>

int parenthesis(int n){
 int ans=parenthesis(n-1);
 if(n==1){
 printf("(1");
  return 1;}
 else{
  if(n%2==1){
  printf("(");
  return ans*n;
   printf("*%d", n);
   }
  else if(n%2==0){
   return ans+n;
   printf("+%d)", n);}
 }
}

int main(void){
int x;
scanf("%d", x);
int y= parenthesis(x);

printf("=%d", y);

return 0;}


