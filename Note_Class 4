//tower of hanoi
#include<stdio.h>

int main(void){
  int n;
  printf();
  scanf();
  hanoi('1', '3', '2', n);
return 0;}

void hanoi(char a, char b, char c, int n){
  if(n==0){
    return 0;}

  hanoi(a, c, b, n-1);
  printf("from tower %c to %c\n", a, b);
  hanoi(c, b, a, n-1);
}







