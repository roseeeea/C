#include<stdio.h>

long long fatorial(long long);

int main(void){

int  n;

scanf("%d", &n);
printf("%lld\n", fatorial(n));
return 0;
}

long long fatorial(long long n){
if (n>0){
return n*fatorial(n-1);
}else{
return 1;
}
}
