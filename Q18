#include<stdio.h>

int main(void){
    char x[8];
    char y[8];
    char z[8];
    y[3]='.'; y[7]='\0';
    z[3]='.'; z[7]='\0';
    
    scanf("%s", x);
    
    for(int i=0; i<3; i++){
        y[i]=x[2-i];
        z[i]=x[i]+y[i]-'0';
    }
    
    for(int i=4; i<7; i++){
        y[i]=x[10-i];
        z[i]=x[i]+y[i]-'0';
    }
    
    for(int i=6; i>0; i--){
        if(z[i]!='.' && i!=4){
            if(z[i]>'9'){
                z[i-1]=z[i-1]+1;
                z[i]=z[i]-10;
            }
        }
        else if(i==4){
            if(z[i]>'9'){
                z[i-2]=z[i-2]+1;
                z[i]=z[i]-10;
            }
        }
    }
    
    if(z[0]>'9'){
        int k=1;
        z[0]= z[0]-10;
        printf("%d%s\n", k, z);
    }
    else{
        printf("%s\n", z);
    }
    
    return 0;
}
