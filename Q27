//havent dealed the >3 points
#include<stdio.h>

int M(int x1, int y1, int x2, int y2, int x3, int y3){
    if((y2-y1)*(x3-x1)-(y3-y1)*(x2-x1)==0){
        return 1;
    }
    else{
        return 0;
    }
}

int main(void){
    int t;
    scanf("%d", &t);
    
    while(t--){
        int n;
        scanf("%d", &n);
        int a[n][2];
        
        for(int i=0; i<n; i++){
            for(int j=0; j<2; j++){
                scanf("%d", &a[i][j]);
            }
        }
        int ans=0;
        for(int i=0; i<n; i++){
            for(int j=i+1; j<n; j++){
                for(int k=j+1; k<n; k++){
                        if(M(a[i][0], a[i][1], a[j][0], a[j][1], a[k][0], a[k][1])){
                            ans++;
                        }
                }
            }
        }
        printf("%d\n", ans);
    }
    return 0;
}


