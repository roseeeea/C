#include<stdio.h>

int ms[1000000];
int s=0;
int mh[1000000];
int h=0;


int M(int x1, int y1, int x2, int y2, int x3, int y3){
    int a=0;
    if((y2-y1)*(x3-x1)-(y3-y1)*(x2-x1)==0){
        if((x2-x1)!=0){
            if(s!=0){
            for(int i=0; i<s; i++){
                if(ms[i]==(y2-y1)/(x2-x1)){
                    a= 0;
                    break;
                }
                if(i==s-1){
                    ms[s]=(y2-y1)/(x2-x1);
                    s++;
                    a= 1;
                }
            }
            }
            else{
                ms[s]=(y2-y1)/(x2-x1);
                s++;
                a= 1;
            }
        }
        else{
            if(h!=0){
            for(int i=0; i<h; i++){
                if(x1==mh[i]){
                    a= 0;
                    break;
                }
                if(i==h-1){
                    mh[h]=x1;
                    h++;
                    a= 1;
                }
            }
            }
            else{
                mh[h]=x1;
                h++;
                a= 0;
            }
        }
    }
    else{
        a= 0;
    }
    return a;
}


int main(void){
    int t;
    scanf("%d", &t);
    
    while(t--){
        int ans=0;
        
        int n;
        scanf("%d", &n);
        int a[n][2];
        
        for(int i=0; i<n; i++){
            for(int j=0; j<2; j++){
                scanf("%d", &a[i][j]);
            }
        }
        
        for(int i=0; i<n; i++){
            for(int j=i+1; j<n; j++){
                for(int k=j+1; k<n; k++){
                        if(M(a[i][0], a[i][1], a[j][0], a[j][1], a[k][0], a[k][1])==1){
                            ans=s+h;
                        }
                }
            }
        }
        
        //test
        for(int i=0; i<s; i++){
            printf("%d ", ms[i]);
        }
        
        printf("%d\n", s+h);
        s=0;
        h=0;
    }
    return 0;
}


