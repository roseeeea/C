#include<stdio.h>

int main(void){
    int t;
    scanf("%d", &t);
    while(t--){
        int n, m, k;
        scanf("%d%d%d", &n, &m, &k);
        
        int N[n];
        for(int i=0;i<n; i++){
            scanf("%d", &N[i]);
        }
        int range[n];
        int x=0;
        int l=0, r=n-1;
        
        //k=k-n-m;
        while(k>0){
            if(N[l]>=N[r]){
                range[x]=N[r];
                r--;
                k--;
                x++;
            }
            else if(N[l]<N[r]){
                range[x]=N[l];
                l++;
                k--;
                x++;
            }
        }
        
        while(x<n){
            if(N[l]<=N[r]){
                range[x]=N[r];
                r--;
                x++;
            }
            else if(N[l]>N[r]){
                range[x]=N[l];
                l++;
                x++;
            }
        }
       /* for(int i=0; i<n; i++){
            printf("%d ", range[i]);
        }
        */
        printf("%d\n", range[m-1]);
    
        
        
    }
    return 0;
}
