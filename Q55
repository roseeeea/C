#include<stdio.h>

int main(void){
    int letter;
    scanf("%d", &letter);
    char s[letter+1];
    scanf("%s", s);
    
    int l=0,r=0;
    for(int i=0; i<=letter; i++){
        if(s[i]=='L'){
            l++;
        }
        else if(s[i]=='R'){
            r++;
        }
    }
    
    printf("%d\n", l+r+1);
    
    return 0;
}

