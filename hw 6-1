#include<stdio.h>

int n;
int a[10000000];
int k;
int ans=0;


void collect(int b, int j){
 for(int s=j+1; s<n; s++){
    int A=b+a[s];
     if(A<k){
        int J=s;
        collect(A, J);
     }
     else if(A==k){
         ans++;
     }
 }
}


int main(void){
    scanf("%d%d", &n, &k);
    
    for(int i=0; i<n; i++){
        scanf("%d", &a[i]);
    }
    
    for(int i=0; i<n; i++){
        if(a[i]==k){
            ans++;
        }
        else if(a[i]<k){
            collect(a[i], i);
        }
    }
    
    printf("%d\n", ans);
}

