#include<stdio.h>

int main(void)
{
    int x1,x2, y1,y2;
    scanf("%d.%d %d.%d", &x1, &x2, &y1, &y2);
    int X= x1*100+x2;
    int Y= y1*100+y2;
    printf("%d.%04d\n", (X*Y)/10000, (X*Y)%10000); \\04表示想要小數點後四位，少的用零表示
    return 0;
}



