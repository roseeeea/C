#include<stdio.h>
#include<string.h>

int j(char a[100], char b[100]){
    int x=0;
    int len = strlen(a);
    if(len ==2){
        if(a[0]==b[0] && a[1]==b[1]){
            x=1;
        }
        else if(a[0]==b[1] && b[1]==a[0]){
            x=1;
        }
    }
    else{
        char a1[len/2], a2[len/2], b1[len/2], b2[len/2];
        for(int i=0; i<len/2; i++){
            a1[i]=a[i];
            b1[i]=b[i];
            a2[i]=a[len+i];
            b2[i]=b[len+i];
        }
        int x1 = j(a1, b1)&j(a2, b2);
        int x2 = j(a1, b2)&j(a2, b1);
        x= x1|x2;
    }
    return x;
}



int main(void){
    char a[100], b[100];
    scanf("%s", a);
    scanf("%s", b);
    
    int len;
    len = strlen(a);
    int ans=1;
    
    if(len%2==1){
        for(int i=0; i<len; i++){
            if(a[i]!=b[i]){
                ans=0;
                break;
            }
        }
    }
    else{
        ans=j(a, b);
        
    }
    
    if(ans==1){
        printf("YES\n");
    }
    else{
        printf("NO\n");
    }
    

    
    
}
