//9*9 matrix
#include<stdio.h>

int main(void){
    
    for(int i=1; i<10; i++){
        for(int j=1; j<10; j++){
            printf("%3d ", i*j);
        }
        printf("\n");
    }
    
    return 0;
}

//right-uppon triangle
#include<stdio.h>

int main(void){
    
    for(int i=1; i<10; i++){
        for(int j=1; j<i; j++){
            printf("    ");
        }
        for(int j=i; j<10; j++){
            printf("%3d ", i*j);
        }
        printf("\n");
    }
    
    return 0;
}

//left-uppon triangle
#include<stdio.h>

int main(void){
    
    for(int i=1; i<10; i++){
        for(int j=1; j<=10-i; j++){
            printf("%3d ", i*j);
        }
        printf("\n");
    }
    
    return 0;
}

//right-downward triangle
#include<stdio.h>

int main(void){
    for(int i=1; i<10; i++){
        for(int j=1; j<10-i; j++){
                    printf("    ");
                }
        for(int j= 10-i; j<10; j++){
            printf("%3d ", i*j);
        }
        printf("\n");
    }
    
    return 0;
}

//left-downward triangle
#include<stdio.h>

int main(void){
    for(int i=1; i<10; i++){
        for(int j= 1; j<i; j++){
            printf("%3d ", i*j);
        }
        printf("\n");
    }
    
    return 0;
}
